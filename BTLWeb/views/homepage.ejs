<% include template/body1 %>
<div id = "container">
    <!-- header starts here-->
<% include template/header %>
<div id ="container_inner">
    <!-- nav starts here-->
<div id="container_inner_nav">
<div id="container_inner_nav-inner">
<% include template/nav %>
</div>
</div>
    <!-- content starts here -->
<div id = "container_inner_content">
<div id ="container_inner_content-inner">
        <div id="main-1">
                <div class="title-sp">
                    <label>Danh sách sản phẩm</label>
                  </div>
                  <div class="cuon-1" id =" frame-1">
                    <table>
                            <tr>
                              <% MayTinh.forEach((data)=>{%>
                                <th>
                                    <% data.files.forEach(function(file) { %>
                                      <div class="card card-body mb-3">
                                        <% if(file.isImage) { %>
                                          <a href="/sp/sp/<%-data.resu._id%>">  <img src="image/<%= file.filename %>" alt="" width="150px" height="150px"></a>
                                          <% } %>
                                      </div>
                                      <% }) %>
                                <span class ="ten" id = "ten-1"><%= data.resu.name%></span><br>
                                <span class ="gia" id = "gia-1"><%= data.resu.price%></span><br>    
                              </th>
                              <% }) %>
                            </tr>
                    </table>
                  </div>
                  <div class="title-sp">
                      <label>Danh sách sản phẩm</label>
                    </div>
                  <div class="cuon-1" id =" frame-2">
                      <table>
                              <tr>
                                <% Chuot.forEach((data)=>{%>
                                  <th>
                                      <% data.files.forEach(function(file) { %>
                                        <div class="card card-body mb-3">
                                          <% if(file.isImage) { %>
                                           <a href="/sp/sp/<%-data.resu._id%>"> <img src="image/<%= file.filename %>" alt="" width="150px" height="150px"></a>
                                            <% } %>
                                        </div>
                                        <% }) %>
                                  <span class ="ten" id = "ten-1"><%= data.resu.name%></span><br>
                                  <span class ="gia" id = "gia-1"><%= data.resu.price%></span><br>    
                                </th> 
                                  <% }) %>
                              </tr>
                      </table>
                    </div>
</div>
</div>
</div>
<% include template/popup %>
    <!-- footer starts here-->
<div id = "container_inner_footer">
<% include template/footer %>
</div>
</div>
